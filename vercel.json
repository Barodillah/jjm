{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 30,
            "memory": 1024
        }
    },
    "buildCommand": "npm run build",
    "framework": "vite"
}